	(kysymys, monivastaus, avoin)
values
	('Oletko tyytyväinen?', '2', 'Joskus vähän emmätiiä'),
	('Juusto vai kinkku päällimmäiseksi?', '1', 'Ainoa oikea vastaus'),
	('Clinton vai Trump?', '5', 'En mä ees asu siel'),
	('Äiti vai tytär?', '2', '...');