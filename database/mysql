CREATE TABLE kysely (
	id SMALLINT NOT NULL AUTO_INCREMENT PRIMARY KEY,
	kysymys VARCHAR(255) NOT NULL,
	monivastaus int(255),
	avoin VARCHAR(255)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


-- MUISTA! console encoding latin1

